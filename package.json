{
  "name": "react-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm run build\" \"node ./bin/www\"",
    "heroku-postbuild": "cd client && npm --production=false && npm run build",
    "build" : "cd client && npm run build",
    "client": "cd client && npm run start",
    "server": "nodemon ./bin/www",
     "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
